{
  "name": "meteor-tools",
  "main": "./lib/meteor-tools",
  "version": "0.0.0",
  "description": "This package provides some tools to control the meteor framework from Atom.io.",
  "keywords": [
    "meteor",
    "development"
  ],
  "activationCommands": {
    "atom-workspace": [
        "meteor-tools:toggleConsole",
        "meteor-tools:createNewProject"
      ]
  },
  "repository": "https://github.com/maprix/meteor-tools.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3"
  }
}
